{
    "name": "osb-syntax",
    "displayName": "osb!Syntax",
    "description": "Syntax and auto-completion tool for osu .osb files",
    "version": "1.0.0",
    "publisher": "OSBTT",
    "repository": {
        "type": "git",
        "url": "https://github.com/S-Traut/Osb-Syntax"
    },
    "icon": "logo.png",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "main": "extension.js",
    "activationEvents": [
        "onLanguage:osb"
    ],
    "dependencies": {
        "vscode": "^1.1.37"
    },
    "contributes": {
        "languages": [
            {
                "id": "osb",
                "aliases": [
                    "Osu!Storyboard language",
                    "osb"
                ],
                "extensions": [
                    ".osb"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "osb",
                "scopeName": "source.osb",
                "path": "./syntaxes/osb.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "osb",
                "path": "./snippets/osb-snippets-general.json"
            },
            {
                "language": "osb",
                "path": "./snippets/osb-snippets-easings.json"
            },
            {
                "language": "osb",
                "path": "./snippets/osb-snippets-parameters.json"
            }
        ]
    },
    "devDependencies": {
        "@types/mocha": "^8.0.3",
        "mocha": "^8.1.1",
        "@types/node": "^12.12.0",
        "eslint": "^6.4.0",
        "@typescript-eslint/parser": "^2.3.0",
        "typescript": "^4.0.2"
    }
}
